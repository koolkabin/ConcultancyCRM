    <div class="col-sm-4">
        <section id="leadAssignments">
            <table class="table table-stripped">
                <tr>
                    <th>SN</th>
                    <th>Date</th>
                    <th>Employee</th>
                </tr>
                @foreach (var item in Model.AssignedLeads)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@ViewHelper.GetDate(item.AssignedDate)</td>
                        <td>@item.Employee.Name</td>
                    </tr>
                }
            </table>
        </section>
        <section id="NewLeadAssignments">
            @{
                await Html.PartialAsync("Partial/_NewLeadAssignForm", Model);
            }
        </section>
        <section id="leadComments">
            <table class="table table-stripped">
                <tr>
                    <th>SN</th>
                    <th>Date</th>
                    <th>Employee</th>
                    <th>Status</th>
                    <th>Comments</th>
                </tr>
                @foreach (var item in Model.LeadComments)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@ViewHelper.GetDate(item.TxnDate)</td>
                        <td>@item.EmpName</td>
                        <td>@item.Status</td>
                        <td>@item.Description</td>
                    </tr>
                }
            </table>
        </section>
        <section id="newLeadComment">
            @{
                await Html.PartialAsync("Partial/_NewLeadCommentForm", Model);
            }
        </section>
    </div>
